{
  "name": "@gethinode/mod-leaflet",
  "version": "0.1.0",
  "description": "Adds Leaflet as Hugo module to your Hinode site (work in progress)",
  "keywords": [
    "hugo",
    "module"
  ],
  "main": "index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "postinstall": "cpy node_modules/leaflet/dist/leaflet.{css,js} dist --flat && cpy node_modules/leaflet/dist/images/** dist/images --flat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gethinode/mod-leaflet.git"
  },
  "author": "Mark Dumay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gethinode/hinode/issues"
  },
  "homepage": "https://gethinode.com",
  "devDependencies": {
    "cpy-cli": "^5.0.0",
    "leaflet": "^1.9.4"
  }
}
