{
  "name": "@gethinode/mod-leaflet",
  "version": "0.5.0",
  "description": "Add Leaflet as Hugo module to your Hinode site (work in progress)",
  "keywords": [
    "hugo",
    "theme",
    "bootstrap",
    "responsive",
    "front-end",
    "blog",
    "documentation"
  ],
  "main": "index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "npm run -s lint:scripts && npm run -s lint:styles && npm run -s lint:markdown",
    "lint:scripts": "eslint assets/js",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint-cli2 \"*.md\" \"content/**/*.md\"",
    "lint:markdown-fix": "markdownlint-cli2-fix \"*.md\" \"content/**/*.md\"",
    "test": "npm run -s lint",
    "env": "hugo env",
    "precheck": "npm version",
    "check": "hugo version",
    "upgrade": "npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gethinode/mod-leaflet.git"
  },
  "author": "Mark Dumay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gethinode/hinode/issues"
  },
  "homepage": "https://gethinode.com",
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "leaflet": "^1.9.4",
    "markdownlint-cli2": "^0.8.1",
    "stylelint": "^15.6.0",
    "stylelint-config-standard-scss": "^10.0.0"
  },
  "hugo-bin": {
    "buildTags": "extended"
  }
}
